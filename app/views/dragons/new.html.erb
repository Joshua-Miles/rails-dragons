<%= form_with model:@dragon do |f| %>
<div>
<label>Name</label>
<%= f.text_field :name %>
</div>

<div>
<label>Breed</label>
<%= f.text_field :breed %>
</div>

<div>
<p>Add a user to your Dragon (required)</p>
<%= f.collection_select :user_id, User.all, :id, :name %>
</div>

<%= f.submit%>
<% end %>